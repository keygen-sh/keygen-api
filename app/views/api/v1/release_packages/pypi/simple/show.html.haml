-# frozen_string_literal: true
- title = "Versions for #{package.name} by #{account.name}"
- content_for(:title) { title }

%h1= title

- package.artifacts.each do |artifact|
  - url = url_for([:v1, account, artifact, anchor: checksum_for(artifact, delimiter: '=')])

  = link_to(artifact.filename, url, data: artifact.metadata)
